{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": {
            "options": "match-declaration-order"
        },
        "array-type": [
            true,
            "array"
        ],
        "max-line-length": [false],
        "no-console": [false, "log", "error"],
        "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}
